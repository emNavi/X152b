<launch>
    <arg name="drone_id" default="0"/>
    <arg name="drone_type" default="ACM0"/>

    <!-- 1. Start mavros -->
	<include file="$(find mavros)/launch/px4.launch">
        <arg name="fcu_url" value="/dev/tty$(arg drone_type):921600"/>
        <arg name="gcs_url" value="udp://:14556@10.42.0.173:14550"/>
    </include>

    <!-- 2. Start realsense camera driver -->
	<include file="$(find camera_driver)/launch/rs_d430_camera.launch"></include>
	<!-- <include file="$(find camera_driver)/launch/rs_d435i_camera.launch"></include> -->

    <!-- 3. Start quadrotor swarm system -->
